{
  "name": "egoi-admin",
  "vendor": "egoipartnerpt",
  "version": "1.11.0",
  "title": "E-goi",
  "description": "E-goi app for VTEX IO ",
  "categories": [],
  "dependencies": {
    "vtex.styleguide": "9.x",
    "vtex.pixel-interfaces": "1.x"
  },
  "builders": {
    "node": "6.x",
    "docs": "0.x",
    "admin": "0.x",
    "react": "3.x",
    "pixel": "0.x",
    "graphql": "1.x",
    "messages": "1.x",
    "store": "0.x"
  },
  "billingOptions": {
    "type": "free",
    "availableCountries": [
      "*"
    ],
    "support": {
      "url": "https://helpdesk.e-goi.com",
      "phone": "+351300404336"
    }
  },
  "scripts": {
    "postreleasy": "vtex publish"
  },
  "credentialType": "absolute",
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "httpstat.us",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "dev-api.egoiapp.com",
        "path": "/lists/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "dev-api.egoiapp.com",
        "path": "/my-account"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "dev-goidini.e-goi.com",
        "path": "/vtex-io/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.egoiapp.com",
        "path": "/lists/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.egoiapp.com",
        "path": "/my-account"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "goidini.e-goi.com",
        "path": "/vtex-io/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "app.io.vtex.com",
        "path": "/api/dataentities/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "/api/vlm/account/stores"
      }
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/dataentities/*"
      }
    },
    {
      "name": "update-app-settings"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
